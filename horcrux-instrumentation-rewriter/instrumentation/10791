<script>(function __HORCRUX__(){ if ('serviceWorker' in window.navigator) {
          window.CBSNEWS.features.executeIfEnabled(['serviceWorker'], function () {
            window.navigator.serviceWorker.register('https://www.cbsnews.com/serviceworker.js')
          }, window);
        }

         window.CBSNEWS = window.CBSNEWS || {};

        if (/(iphone|ipad|ipod|android)/i.test(window.navigator.userAgent) && (!/SB_download/.test(window.document.cookie) || window.CBSNEWS.features.isFeatureDisabled('beforeinstallprompt'))) {
          window.addEventListener("beforeinstallprompt", function(e) {
            // Prevent Chrome 67 and earlier from automatically showing the prompt
            e.preventDefault();
            // Stash the event so it can be triggered later.
            window.CBSNEWS.deferredPrompt = e;
            // stash it but don't use it so that native PWA banner doesn't crop up
          });
        } })();</script></body></html>
