     (function __HORCRUX__(){ window.SUPPORTS_PAGE_TRANSITION_EVENTS = 'onpageshow' in window;
     window.IS_SAFARI = !!(typeof window.safari === 'object' && window.safari.pushNotification);

    if (window.IS_SAFARI) {
        window.addEventListener('beforeunload', function (e) {
            window._safariBeforeUnloadTimeout = window.setTimeout(function () {
                // Fail fast
                if (e.defaultPrevented || e.returnValue.length > 0) return;

                window.cbsnewsOnPageUnload();
            }, 0);
        });
    } else {
        window.addEventListener('pagehide', function (e) {
            // Fail fast
            if (e.persisted) return;

            window.cbsnewsOnPageUnload();
        });
    } })();

    function cbsnewsOnPageUnload (e) {
        // Fail fast
        if (
            !window.CBSNEWS.tracking ||
             window.CBSNEWS.tracking._appMeasurementTrackDone ||
             window.CBSNEWS.tracking.noViewTrack
        ) {
            return;
        }

        var vidCookie = window.document.cookie.match(/fly_vid=([a-z0-9-]+)/);
        if (vidCookie && vidCookie[1]) {
            window.CBSNEWS.tracking.visitorId = vidCookie[1];
        } else {
            window.CBSNEWS.tracking.visitorId = window.uuid.v4();
        }

        window.document.cookie = "fly_vid=" + window.CBSNEWS.tracking.visitorId + ';domain=.' + window.document.domain + ';path=/;max-age=31536000';

        var now = Date.now();
        var rPlaceholder = /__([a-z0-9]+)__/gi;
        var pixel = 'https://saa.cbsnews.com/b/ss/cbsicbsnewssite/1/s?AQB=1&g=__canonicalUrl__&gn=cbsnews%3A__canonicalPath__&c=24&ce=UTF-8&ch=&h1=D%3D%22cbsnews%3A%22%2Bch&ndh=0&server=www.cbsnews.com&vid=__visitorId__&c.=&brandPlatformId=cbsnews_site_mweb&.c=&c2=D%3Dv2&c3=D%3Dv3&c5=D%3Dv5&c6=D%3Dv6&c7=D%3Dv7&c8=D%3Dv8&c9=D%3DUser-Agent&c10=D%3Dv10&c11=D%3Dv11&c20=D%3Dv20&c22=D%3Dv22&c23=D%3Dv23&c24=D%3Dv24&c30=D%3Dv30&v1=cbsnews_site_mweb&v2=us&v3=responsive+web%7C%7C%7Cmobile&v5=cbsicbsnewssite&v6=D%3Dh1&v7=D%3Dg&v8=__articleTitle__&v9=D%3DUser-Agent&v10=&v11=D%3Dch%2B%22%3A%22%2Bv10&v15=not+authenticated%7Canon&v20=notSet&v22=&v23=&v24=__pageViewGuid__&v30=notSet&r=__referrer__&events=event100&AQE=1'.replace(rPlaceholder, function (match) {
            return ({
                __articleTitle__:     window.encodeURIComponent(window.document.title),
                __canonicalPath__:    window.encodeURIComponent(window.location.pathname),
                __canonicalUrl__:     window.encodeURIComponent(window.location.href),
                __pageViewGuid__:     window.CBSNEWS.tracking.pageViewGuid,
                __referrer__:         window.encodeURIComponent(window.document.referrer),
                __time__:             Date.now(),
                __timezoneOffset__:   (new Date()).getTimezoneOffset(),
                __userAgent__:        window.encodeURIComponent(window.navigator.userAgent),
                __visitorId__:        window.CBSNEWS.tracking.visitorId,
                __totalEngagedTime__: (Date.now() - now) / 1000,
